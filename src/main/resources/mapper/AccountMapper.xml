<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qtech.springboottest.mapper.AccountMapper">
    <select id="login" parameterType="com.qtech.springboottest.entity.Account" resultType="com.qtech.springboottest.entity.Account">
        select * from user where username = #{username} and password = #{password}
    </select>


</mapper>